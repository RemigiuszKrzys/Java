/** Napisz metode która sprawdzi czy łancuch znaków jest palindromem
 *
 */
public class Main {
    public static void main(String[] args) {

    System.out.println(isPalindrome( "tekst"));
    System.out.println(isPalindrome( "teet"));
    System.out.println(isPalindrome( "oko w oko"));
    }
    static boolean isPalindrome(String s){
        for(int i = 0; i != s.length() / 2; i++)
if (s.charAt(i) != s.charAt(s.length() - 1 - i))
    return false;
            return true;
    }

}
